    "# N-Gram"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "4f41e9dd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "('India', 'is', 'is')\n",
      "('is', 'is', 'my')\n",
      "('is', 'my', 'country.All')\n",
      "('my', 'country.All', 'Indians')\n",
      "('country.All', 'Indians', 'are')\n",
      "('Indians', 'are', 'my')\n",
      "('are', 'my', 'brothers')\n",
      "('my', 'brothers', 'and')\n",
      "('brothers', 'and', 'sisters')\n"
     ]
    }
   ],
   "source": [
    "import nltk\n",
    "from nltk import ngrams\n",
    "sentence=\"India is is my country.All Indians are my brothers and sisters\"\n",
    "words=sentence.split()\n",
    "my_ngrams=ngrams(words,3)\n",
    "for n in my_ngrams:\n",
    "    print(n)"
   ]
